{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/Data": ["src/data/*"],
      "@/Utils": ["src/utils/*"],
      "@/Assets*": ["src/assets/*"],
      "@/Components": ["src/components/*"],
      "@/CommonLess": ["src/commonLess.less"]
    },
    "allowSyntheticDefaultImports": true
  }
}
